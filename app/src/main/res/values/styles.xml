<resources>
    <style name="AppTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="actionBarStyle">@style/ActionBar.Solid.Earthquake</item>
    </style>
    <style name="EarthquakeTheme" parent="AppTheme">
        <item name="actionBarStyle">@style/ActionBar.Solid.Earthquake.NoTitle</item>
    </style>
    <style name="ActionBar.Solid.Earthquake"
        parent="@style/Widget.AppCompat.Light.ActionBar.Solid.Inverse">
        <item name="background">@color/action_barColor</item>
    </style>
    <style name="ActionBar.Solid.Earthquake.NoTitle" parent="@style/ActionBar.Solid.Earthquake">
        <item name="displayOptions">useLogo|showHome</item>
        <item name="logo">@drawable/ic_launcher</item>
    </style>




</resources>
